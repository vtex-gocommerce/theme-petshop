{
  "store.home": {
    "blocks": ["carousel#home", "shelf#home", "shelf#home"]
  },
  "store.product": {
    "blocks": ["product-details#default", "product-kit", "shelf.relatedProducts"]
  },
  "store.search": {
    "blocks": ["search-result"]
  },
  "store.search#brand": {
    "blocks": ["search-result"]
  },
  "store.search#department": {
    "blocks": ["search-result#department"]
  },
  "store.search#category": {
    "blocks": ["search-result"]
  },
  "store.search#subcategory": {
    "blocks": ["search-result"]
  },
  "store.account": {
    "blocks": ["my-account"],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.login": {
    "blocks": ["login-content#default"]
  },
  "header.full": {
    "blocks": ["login", "minicart", "logo", "search-bar", "menu-link", "telemarketing", "category-menu"],
    "props": {
      "logoUrl": "https://static.gocommerce.com/gc-prw6252/assets/gocommerce/png/petshop-logo___fa866ef7a6118f19f97c5ee98759b279.png"
    }
  },
  "login": {
    "props": {
      "showPasswordVerificationIntoTooltip": true
    }
  },
  "menu-link": {
    "props": {
      "links": [
        {
          "text": "Contact Us",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
