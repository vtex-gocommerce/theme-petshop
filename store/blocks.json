{
  "store.home": {
    "blocks": ["carousel#home", "shelf#home"]
  },
  "store.product": {
    "blocks": ["product-details#default", "product-kit", "shelf.relatedProducts"]
  },
  "store.search": {
    "blocks": ["search-result"]
  },
  "store.search#brand": {
    "blocks": ["search-result"]
  },
  "store.search#department": {
    "blocks": ["search-result#department"]
  },
  "store.search#category": {
    "blocks": ["search-result"]
  },
  "store.search#subcategory": {
    "blocks": ["search-result"]
  },
  "store.account": {
    "blocks": ["my-account"],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.login": {
    "blocks": ["login-content#default"]
  },
  "header.full": {
    "blocks": ["login", "minicart", "logo", "search-bar", "menu-link", "telemarketing", "category-menu"],
    "props": {
      "logoUrl": "https://static.gocommerce.com/{{account}}/assets/gocommerce/png/logo.png"
    }
  },
  "login": {
    "props": {
      "showPasswordVerificationIntoTooltip": true
    }
  },
