{
  "store.home": {
    "blocks": ["carousel#home", "shelf#home"]
  },
  "store.product": {
    "blocks": ["product-details#default", "product-kit", "shelf.relatedProducts"]
  },
  "store.search": {
    "blocks": ["search-result"]
  },
  "store.search#brand": {
    "blocks": ["search-result"]
  },
  "store.search#department": {
    "blocks": ["search-result#department"]
  },
  "store.search#category": {
    "blocks": ["search-result"]
  },
  "store.search#subcategory": {
    "blocks": ["search-result"]
  },
  "store.account": {
    "blocks": ["my-account"],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.login": {
    "blocks": ["login-content#default"]
  },
  "header.full": {
    "blocks": ["login", "minicart", "logo", "search-bar", "telemarketing", "category-menu"],
    "props": {
      "logoUrl": "https://static.gocommerce.com/gc-wqp6166/assets/gocommerce/png/logo-zen___d7d86cabd178c600ec16d80be67d93ac.png"
    }
  },
  "login": {
    "props": {
      "showPasswordVerificationIntoTooltip": true
    }
  },
  "menu-link": {
    "props": {
      "links": [
        {
          "text": "Contact Us",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
          "position": "left"
        },
        {
          "text": "My Requests",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
          "position": "left"
        },
        {
          "text": "My Account",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
          "position": "left"
        },
        {
          "text": "Wishlist",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
          "position": "right"
        },
        {
          "text": "Support",
          "typeOfRoute": "internal",
          "internalPage": "store.home",
          "position": "right"
        }
      ]
    }
  },
  "category-menu": {
    "props": {
      "showAllDepartments": false
    }
  },
